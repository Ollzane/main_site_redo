<div id="articles-container">
  <div id="articles-wrapper">
    <h1 id="page-title">Stuff that I've written</h1>
    <% @posts.each do |post| %>
      <div class="article-post">
        <p class="date"><%= post.created_at.strftime('%A, %B %d') %></p>
        <h2><a href=""><%= link_to post.title, post %></a></h2>
        <hr class="post-sep">
      </div>
    <% end %>
  </div>

  <%= will_paginate @posts %>
</div>
